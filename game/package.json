{
  "name": "helloworld",
  "version": "0.0.1",
  "description": "video-conferencing for everybody!",
  "author": "Blake La Pierre",
  "dependencies": {
    "express": "latest",
    "socket.io": "latest",
    "webrtc.io": "latest",
    "webrtc.io-client": "latest",
    "nodemailer": "latest"
  },
  "devDependencies": {
    "grunt": "latest",
    "grunt-bg-shell": "latest",
    "browserify": "3.33.0",
    "browserify-shim": "3.3.2",
    "partialify": "latest",
    "uglifyify": "latest"
  },
  "browserify": {
  	"transform": [
  		"browserify-shim",
  		"partialify",
      "uglifyify"
  	]
  },
  "browserify-shim": {
  	"angular": "global:angular",
  	"three": "global:THREE",
    "stats": "global:Stats",
    "webrtc.io": "global:rtc",
    "socket.io": "global:io",
    "underscore": "global:_",
    "mathjs": "global:mathjs"
  }
}